{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ {
      "source": "/login",
      "function": "app"
    },{
      "source": "/auth/linkedin",
      "function": "app"
    },{
      "source": "/auth/linkedin/callback",
      "function": "app"
    },{
      "source": "/linkedin-test",
      "function": "app"
    } ]
  }
}
